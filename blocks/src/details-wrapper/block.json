{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ucsc/details-wrapper",
	"version": "0.1.0",
	"title": "Details wrapper",
	"category": "text",
	"icon": "button",
	"description": "Wrapper for the details block",
		"keywords": [
			"accordion",
			"faq"
		],
	"attributes": {
		"wrapperButton": {
			"type": "string",
			"default": "Expand All"
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"color": {
			"text": false,
			"background": true
		}
	},
	"textdomain": "details-wrapper",
	"editorScript": "file:./index.js",
	"viewScript": "file./view.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
